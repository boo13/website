{
  "name": "RandyCounsman",
  "version": "1.0.0",
  "description": "Personal website for Randy Counsman",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    // "css:scss": "node-sass --output-style compressed -o dist src/scss",
    "css:autoprefixer": "postcss -u autoprefixer -r dist/*.css",
    "css:lint": "stylelint src/scss/*.scss --syntax scss || true",
    "build:css": "npm run css:lint && npm run css:scss && npm run css:autoprefixer",
    "watch:css": "onchange -d 2000 \"src/scss\" -- npm run build:css",
    "serve": "browser-sync start --server \"dist\" --files \"dist\"",
    "build:images": "imagemin src/images/**/* --out-dir=dist/images",
    "watch:images": "onchange -d 2000 \"src/images\" -- npm run build:images",
    "build:js": "webpack --mode=production",
    "watch:js": "onchange -d 2000 \"src/js\" -- webpack --mode=development",
    "build:html": "posthtml -c posthtml.json",
    "watch:html": "onchange -d 2000 \"src/views\" -- npm run build:html",
    "watch": "run-p serve watch:*",
    "build": "run-p build:*",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [],
  "author": "Randy Counsman",
  "license": "MIT",
  "homepage": "https://boo13.github.io/website",
  "devDependencies": {
    "@babel/preset-env": "^7.14.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "eslint": "^7.25.0",
    "eslint-loader": "^4.0.2",
    "gh-pages": "^3.1.0",
    "htmlnano": "^1.0.0",
    "imagemin-cli": "^6.0.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss-cli": "^8.3.1",
    "posthtml": "^0.15.2",
    "posthtml-cli": "^0.9.1",
    "posthtml-modules": "^0.7.4",
    "stylelint": "^13.13.0",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "@barba/core": "^2.9.7"
  }
}
